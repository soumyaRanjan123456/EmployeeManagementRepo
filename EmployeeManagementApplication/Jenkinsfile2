pipeline{
  agent any{
    stage('Checkout Code')
    {
      steps{
        git'https://github.com/soumyaRanjan123456/EmployeeManagementRepo'
      }
    }
    stage('Build')
    {
      steps{
        sh 'echo "building the app"'
      }
    }
    stage('Test')
    {
      steps{
        sh 'echo"Running tests"'
      }
    }
    stage('Deploy')
    {
      steps{
        sh 'echo "deploying"'
      }
    }
  }
}
